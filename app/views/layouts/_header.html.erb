<nav class="text-center">
  <div class="row">
    <div class="col-sm-3">Flatiron Store</div>
    <div class="col-sm-3">
          <%= link_to "Store Home", store_path %>
    </div>
    <div class="col-sm-3">
        <% if !!@cart %>
          <%= link_to "Cart", cart_path(current_cart) %>
        <% end %>
    </div>
    <div class="col-sm-3">
        <% if current_user %>
          <p>Signed in as <%= current_user.email %></p>
          <p><%= link_to("Sign out", destroy_user_session_path, :method => 'delete') %></p>
        <% else %>
          <p><%= link_to("sign in", new_user_session_path) %></p>
          <p><%= link_to("Sign up", new_user_registration_path) %><br></p>
        <% end %>
    </div>
</nav>
